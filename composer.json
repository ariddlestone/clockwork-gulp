{
    "name": "clockwork/gulp",
    "description": "A Zend Framework 2 module for Gulp configuration",
    "keywords": [
        "clockwork",
        "gulp",
        "zf2"
    ],
    "type": "library",
    "require": {
        "php": "^7.3",
        "clockwork/console": "^1.0",
        "container-interop/container-interop": "^1.2",
        "symfony/console": "^4.3",
        "zendframework/zend-component-installer": "^1.0 || ^0.7",
        "zendframework/zend-servicemanager": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Clockwork\\Gulp\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Clockwork\\Gulp\\Test\\": "test/"
        }
    },
    "extra": {
        "zf": {
            "component": "Clockwork\\Gulp"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Andrew Riddlestone",
            "email": "andrew.riddlestone@gmail.com"
        }
    ]
}
